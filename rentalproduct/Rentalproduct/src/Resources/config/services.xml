<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Rentalproduct\Core\Content\RentalProduct\RentalProductDefinition">
            <tag name="shopware.entity.definition" entity="rental_product" />
        </service>

        <service id="Rentalproduct\Core\Content\Extension\Product\RentalProductExtension">
            <tag name="shopware.entity.extension"/>
        </service>

        <service id="Rentalproduct\Core\Content\Extension\Rule\RentalRuleExtension">
            <tag name="shopware.entity.extension"/>
        </service>
    </services>
</container>
