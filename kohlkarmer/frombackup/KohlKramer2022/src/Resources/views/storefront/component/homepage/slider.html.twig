                    {% block cms_home_page_slider_image %}

                        <div class="slider responsive">
                            {% for key,sliders in results.entities.elements %}
                                {% set sliderImages = searchMedia([sliders.mediaId], context.context) %}
                                {% set sliderImage = sliderImages.get(sliders.mediaId) %}

                                <div>



                                    <div class="row">
                                        {% if sliderImage.url %}
                                        <div class="col-sm-12 col-md-6">
                                            <img src="{{ sliderImage.url }}"/>
                                        </div>
                                        {% endif %}
                                        <div class="col-sm-12 col-md-6">
                                            <div class="heroslider-content">
                                            {% if sliders.translated.title %}
                                                <div class="row"><h2>{{ sliders.translated.title | raw }}</h2></div>
                                            {% endif %}
                                            {% if sliders.translated.description %}
                                                <p>{{ sliders.translated.description | raw }}</p>
                                            {% endif %}
                                            <a href="{{ sliders.translated.link }}">
                                                {% if sliders.translated.linkText and sliders.translated.link %}
                                                    {{ sliders.translated.linkText }}
                                                {% endif %}
                                            </a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            {% endfor %}
                        </div>
                    {% endblock %}



